{
  "name": "checklyhq.com",
  "scope": "checkly",
  "build": {
    "env": {}
  },
  "headers": [
    {
      "source": "/jamstackconf",
      "headers": [
        { "key": "x-xss-protection", "value": "0" },
        { "key": "x-content-type-options", "value": "nosniff" }
      ]
    },
    {
      "source": "/:path*",
      "headers": [
        { "key": "x-frame-options", "value": "deny" },
        { "key": "x-xss-protection", "value": "0" },
        { "key": "x-content-type-options", "value": "nosniff" }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/a/b.js",
      "destination": "https://plausible.io/js/plausible.js"
    },
    {
      "source": "/a/e",
      "destination": "https://plausible.io/api/event"
    },
    {
      "source": "/jamstackconf",
      "destination": "/"
    }
  ]
}
